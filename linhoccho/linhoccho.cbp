<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="linhoccho" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="../incompleted" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/linhoccho" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after="scanner C:\Users\HUST\Desktop\CompileLab\test\kpl_program.kpl" />
				</ExtraCommands>
				<MakeCommands>
					<Build command="$make -f $makefile" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean$target" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile $target" />
					<SilentBuild command="$make -f $makefile &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/linhoccho" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="../incompleted/Makefile" />
		<Unit filename="../incompleted/charcode.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../incompleted/charcode.h" />
		<Unit filename="../incompleted/charcode.o" />
		<Unit filename="../incompleted/error.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../incompleted/error.h" />
		<Unit filename="../incompleted/error.o" />
		<Unit filename="../incompleted/reader.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../incompleted/reader.h" />
		<Unit filename="../incompleted/reader.o" />
		<Unit filename="../incompleted/scanner.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../incompleted/scanner.exe" />
		<Unit filename="../incompleted/scanner.o" />
		<Unit filename="../incompleted/token.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../incompleted/token.h" />
		<Unit filename="../incompleted/token.o" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
